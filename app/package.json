{
	"name": "react-starter",
	"version": "1.0.0",
	"description": "Starter package for a React based single page application.",
	"main": "src/main.js",
	"repository": "git@github.com:Asymmetrik/react-starter.git",
	"author": "Robert Winterbottom <rwinterbottom@asymmetrik.com>",
	"license": "MIT",
	"private": true,
	"prettier": {
		"semi": true,
		"printWidth": 120,
		"singleQuote": true,
		"trailingComma": "all"
	},
	"babel": {
		"plugins": [
			"@babel/plugin-proposal-class-properties",
			"@babel/plugin-syntax-dynamic-import",
			"@babel/plugin-transform-runtime"
		],
		"presets": [
			"@babel/preset-react",
			[
				"@babel/preset-env",
				{
					"useBuiltIns": "entry",
					"corejs": "3.9"
				}
			]
		],
		"sourceType": "unambiguous"
	},
	"jest": {
		"testEnvironment": "node",
		"verbose": true,
		"collectCoverage": true,
		"coverageReporters": [
			"text",
			"lcov",
			"json"
		],
		"coveragePathIgnorePatterns": [
			"<rootDir>/src/plugins/cases",
			"<rootDir>/node_modules"
		]
	},
	"scripts": {
		"test": "jest",
		"dev": "node src/scripts/nodemon.js",
		"start": "node src/scripts/serve.js",
		"linter": "eslint \"src/**/*.js\"",
		"prettier": "prettier \"src/**/*.js\" --write",
		"changelog": "conventional-changelog -p angular -s -i CHANGELOG.md"
	},
	"devDependencies": {
		"@babel/plugin-transform-runtime": "^7.12.1",
		"babel-eslint": "^10.1.0",
		"conventional-changelog-cli": "^2.1.1",
		"eslint": "^7.12.1",
		"eslint-plugin-react": "^7.21.5",
		"jest": "^26.6.3",
		"nodemon": "^2.0.6",
		"prettier": "^2.1.2",
		"webpack-dev-middleware": "^4.0.0",
		"webpack-hot-middleware": "^2.24.4"
	},
	"dependencies": {
		"@babel/core": "^7.12.3",
		"@babel/plugin-proposal-class-properties": "^7.12.1",
		"@babel/plugin-syntax-dynamic-import": "^7.8.3",
		"@babel/preset-env": "^7.12.1",
		"@babel/preset-react": "^7.12.5",
		"@babel/runtime": "^7.12.5",
		"@material-ui/core": "^4.11.0",
		"@material-ui/icons": "^4.9.1",
		"@material-ui/styles": "^4.10.0",
		"autoprefixer": "^10.0.1",
		"aws-sdk": "^2.786.0",
		"babel-loader": "^8.1.0",
		"body-parser": "^1.19.0",
		"compression": "^1.7.4",
		"css-loader": "^5.0.1",
		"express": "^4.16.4",
		"express-session": "^1.17.1",
		"glob": "^7.1.4",
		"helmet": "^4.2.0",
		"immutable": "^4.0.0-rc.12",
		"mini-css-extract-plugin": "^1.2.1",
		"node-sass": "^5.0.0",
		"passport": "^0.4.1",
		"postcss-loader": "^4.0.4",
		"pug": "^3.0.0",
		"react": "^17.0.1",
		"react-dom": "^17.0.1",
		"react-router-dom": "^5.2.0",
		"redux": "^4.0.5",
		"sass-loader": "^10.0.5",
		"style-loader": "^2.0.0",
		"superagent": "^6.1.0",
		"webpack": "^5.4.0",
		"winston": "^3.3.3"
	}
}
